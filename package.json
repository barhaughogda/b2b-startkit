{
  "name": "startkit-business",
  "version": "0.1.0",
  "private": true,
  "description": "B2B SaaS/AaaS Starter Kit - A production-grade foundation for building multiple SaaS products",
  "author": "StartKit",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules",
    "test": "turbo test",
    "create:product": "tsx infra/scripts/create-product.ts"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
