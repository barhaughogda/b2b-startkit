{
  "name": "@startkit/config",
  "version": "0.1.0",
  "private": true,
  "description": "Shared TypeScript configurations and environment validation for StartKit",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./tsconfig/base": "./tsconfig/base.json",
    "./tsconfig/nextjs": "./tsconfig/nextjs.json",
    "./tsconfig/react-library": "./tsconfig/react-library.json",
    "./tsconfig/node-library": "./tsconfig/node-library.json",
    "./env": "./src/env.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
